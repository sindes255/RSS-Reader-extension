{
    "manifest_version": 2,
  "name": "RSS Reader extension",
  "version": "0.1",
  "description": "RSS Reader extension",
    "chrome_url_overrides" : {
        "newtab": "rssReader.html"
    },

  "permissions": [
    "tabs",
      "http://www.adme.ru/rss/metabar",
      "http://news.sportbox.ru/taxonomy/term/11731/0/feed"
  ],
    "externally_connectable": {
        "matches": ["http://www.adme.ru/rss/metabar","http://news.sportbox.ru/taxonomy/term/11731/0/feed"]
    },
  "browser_action": {
    "default_title": "RSS Reader",
    "default_icon": "files/images/rss-feeds.png",
    "default_popup": "rssReader.html" 
  }
}